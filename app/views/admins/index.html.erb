
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Dashboard</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item active"><a href="#">Dashboard</a></li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- /.row -->
        <div class="row">
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">Recents 5 Quotations</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body table-responsive p-0">
                <table class="table table-striped table-bordered" style="width:100%">
                            <thead>
                                <tr>
                                    <th>Quotation ID</th>
                                    <th>Date Create</th>
                                    <th>To</th>
                                    <th>Attn</th>
                                    <th>Quotation Type</th>
                                    <th>Status</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% @quotation.each do |quotations|%>
                                    <tr>
                                        <td><%= quotations.quotation_id %></td>
                                        <td><%= quotations.created_at.strftime("%Y-%m-%d") %></td>
                                        <td><%= quotations.to %></td>
                                        <td><%= quotations.attn %></td>
                                        <td><%= quotations.type_quotation %></td>
                                        <td><%= quotations.status %></td>
                                        <td> 
                                        <div class="btn-group">
                                            <%= link_to '<span>View</span>'.html_safe,  admins_manage_shipment_path(quotations.quotation_id), data: {turbolinks: false}, class: 'btn btn-primary'%>
                                            <%= link_to '<span>Delete</span>'.html_safe,  admins_manage_shipment_path(quotations.quotation_id), :method => :delete, data: { confirm: 'Are you sure you want to delete this quotation?' }, class: 'btn btn-danger '%>
                                        </div>
                                        </td>
                                    </tr>
                                <% end %>
                            </tbody>
              </table>
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">Recents 5 Users</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body table-responsive p-0">
                <table class="table table-striped table-bordered" style="width:100%">
                            <thead>
                                <tr>
                                    <th>No. </th>
                                    <th>Email</th>
                                    <th>Date Create</th>
                                    <th>Name</th>
                                    <th>No Telephone</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% @user.each_with_index do |users, index| %>
                                    <tr>
                                        <td><%= index + 1 %></td>
                                        <td><%= users.email %></td>
                                        <td><%= users.created_at.strftime("%Y-%m-%d") %></td>
                                        <td><%= users.name %></td>
                                        <td><%= users.no_tel %></td>
                                        <td> 
                                        <div class="btn-group">
                                            <%= link_to '<span>View</span>'.html_safe,  admins_manage_user_path(users.id), data: {turbolinks: false}, class: 'btn btn-primary'%>
                                            <%= link_to '<span>Delete</span>'.html_safe,  admins_manage_user_path(users.id), :method => :delete, data: { confirm: 'Are you sure you want to delete this user?' }, class: 'btn btn-danger '%>
                                        </div>
                                        </td>
                                    </tr>
                                <% end %>
                            </tbody>
              </table>
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">Recents 5 Clients</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body table-responsive p-0">
                <table class="table table-striped table-bordered" style="width:100%">
                            <thead>
                                <tr>
                                    <th>No. </th>
                                    <th>Name</th>
                                    <th>Date Create</th>
                                    <th>Company</th>
                                    <th>No Telephone</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% @client.each_with_index do |clients, index|%>
                                    <tr>
                                        <td><%= index + 1 %></td>
                                        <td><%= clients.name %></td>
                                        <td><%= clients.created_at.strftime("%Y-%m-%d") %></td>
                                        <td><%= clients.address %></td>
                                        <td><%= clients.no_tel %></td>
                                        <td> 
                                        <div class="btn-group">
                                            <%= link_to '<span>View</span>'.html_safe,  admins_manage_client_path(clients.id), data: {turbolinks: false}, class: 'btn btn-primary'%>
                                            <%= link_to '<span>Delete</span>'.html_safe,  admins_manage_client_path(clients.id), :method => :delete, data: { confirm: 'Are you sure you want to delete this Client?' }, class: 'btn btn-danger '%>
                                        </div>
                                        </td>
                                    </tr>
                                <% end %>
                            </tbody>
              </table>
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>